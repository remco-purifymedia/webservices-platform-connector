{
  "name": "pfm_webservices_nl/platform-connector",
  "description": "platform connector generator for Webservices.nl",
  "keywords": [
    "soap",
    "soapclient",
    "webservicesNl",
    "webservices.nl"
  ],
  "config": {
    "gitlab-domains": [
      "gitlab.fdmg.org"
    ],
    "preferred-install": "dist"
  },
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "WebservicesNL",
      "email": "tech@webservices.nl",
      "homepage": "http://www.webservices.nl"
    }
  ],
  "prefer-stable": true,
  "require": {
    "php": "*",
    "ext-soap": "*",
    "ext-xml": "*",
    "ext-dom": "*",
    "guzzlehttp/guzzle": "*",
    "psr/log": "*",
    "pfm_webservices_nl/common": "*",
    "pfm_webservices_nl/utils": "*",
    "roave/security-advisories": "dev-latest"
  },
  "require-dev": {
    "phpunit/phpunit": "*",
    "mockery/mockery": "*",
    "monolog/monolog": "*",
    "league/factory-muffin": "*",
    "friendsofphp/php-cs-fixer": "*",
    "sensiolabs/security-checker": "*",
    "roave/security-advisories": "*",
    "squizlabs/php_codesniffer": "*"
  },
  "autoload": {
    "psr-4": {
      "WebservicesNl\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "WebservicesNl\\Test\\": "test/"
    }
  }
}
